{
  "private": false,
  "name": "@launchdarkly/toolbar",
  "version": "1.1.0",
  "description": "A React component that provides a developer-friendly toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "react",
    "toolbar",
    "development",
    "typescript"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/js/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "cdn",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/js/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "pnpm run build:lib && pnpm run build:cdn",
    "build:lib": "rslib build",
    "build:cdn": "rslib build --config rslib.config.cdn.ts",
    "dev": "rslib build --watch",
    "dev:cdn": "rslib build --config rslib.config.cdn.ts --watch",
    "test": "vitest run",
    "lint": "oxlint",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.9.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.2",
    "@launchpad-ui/components": "^0.16.12",
    "@launchpad-ui/tokens": "^0.15.1",
    "@lezer/highlight": "^1.2.1",
    "@react-aria/focus": "^3.21.2",
    "@react-stately/flags": "^3.1.2",
    "@rsbuild/core": "^1.6.0",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.16.1",
    "@storybook/addon-docs": "^10.0.1",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/addon-onboarding": "^9.0.5",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/react": "^9.1.15",
    "@storybook/react-vite": "^9.0.5",
    "@storybook/test": "^9.0.0-alpha.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "@vitest/coverage-v8": "4.0.5",
    "css-loader": "^7.1.2",
    "jsdom": "^27.0.1",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "motion": "^12.19.2",
    "oxlint": "^1.24.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^9.1.16",
    "storybook-addon-rslib": "^2.1.2",
    "storybook-react-rsbuild": "^2.1.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "launchdarkly-js-client-sdk": ">=3.9.0 <4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
