{
  "private": false,
  "name": "@launchdarkly/toolbar",
  "version": "0.24.0-beta.1",
  "description": "A React component that provides a developer-friendly toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "react",
    "toolbar",
    "development",
    "typescript"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "cdn",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "pnpm run build:npm && pnpm run build:cdn",
    "build:npm": "vite build",
    "build:cdn": "BUILD_TARGET=cdn vite build",
    "dev": "vite build --watch",
    "dev:cdn": "BUILD_TARGET=cdn vite build --watch",
    "test": "vitest run",
    "lint": "oxlint",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "@launchpad-ui/components": "0.16.12",
    "@launchpad-ui/tokens": "0.15.1",
    "@react-aria/focus": "3.21.2",
    "@react-aria/interactions": "3.25.6",
    "@react-aria/utils": "3.31.0",
    "@react-stately/flags": "^3.1.2",
    "@react-stately/utils": "^3.10.8",
    "@react-types/shared": "3.32.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@vanilla-extract/css": "^1.17.4",
    "motion": "^12.19.2",
    "react": "19.2.0",
    "react-aria": "3.44.0",
    "react-aria-components": "1.13.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.59.0",
    "react-router": "7.5.2"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.5",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^9.0.5",
    "@storybook/addon-onboarding": "^9.0.5",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/react": "^9.0.5",
    "@storybook/react-vite": "^9.0.5",
    "@storybook/test": "^9.0.0-alpha.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.4",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^26.1.0",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "oxlint": "^1.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storybook": "^9.1.10",
    "typescript": "^5.8.3",
    "vite": "5.4.7",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-dts": "4.0.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "launchdarkly-js-client-sdk": "^3.9.0",
    "react": ">=18"
  }
}
