{
  "private": false,
  "name": "@launchdarkly/toolbar",
  "version": "0.24.0-beta.1",
  "description": "A React component that provides a developer-friendly toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "react",
    "toolbar",
    "development",
    "typescript"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest run",
    "lint": "oxlint"
  },
  "dependencies": {
    "@launchdarkly/toolbar-types": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "react": ">=18",
    "typescript": "^5.8.3",
    "vite-plugin-dts": "4.0.1",
    "vite": "5.4.7"
  },
  "peerDependencies": {
    "react": ">=18",
    "launchdarkly-js-client-sdk": "^3.9.0"
  }
}