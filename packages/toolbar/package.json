{
  "private": false,
  "name": "@launchdarkly/toolbar",
  "version": "2.0.0",
  "description": "A framework-agnostic developer toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "toolbar",
    "react",
    "development",
    "typescript",
    "javascript",
    "vanilla-js"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/js/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugins": [
        "./dist/plugins.d.ts"
      ],
      "react": [
        "./dist/react.d.ts"
      ],
      "types": [
        "./dist/types-entry.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "cdn",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/js/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins.d.ts",
        "default": "./dist/js/plugins.js"
      },
      "require": {
        "types": "./dist/plugins.d.ts",
        "default": "./dist/plugins.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/js/react.js"
      },
      "require": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types-entry.d.ts",
        "default": "./dist/js/types-entry.js"
      },
      "require": {
        "types": "./dist/types-entry.d.ts",
        "default": "./dist/types-entry.cjs"
      }
    }
  },
  "scripts": {
    "build": "pnpm run build:lib && pnpm run build:cdn",
    "build:lib": "rslib build",
    "build:cdn": "rslib build --config rslib.config.cdn.ts",
    "dev": "rslib build --watch",
    "dev:cdn": "rslib build --config rslib.config.cdn.ts --watch",
    "test": "vitest run",
    "lint": "oxlint",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/language": "^6.12.1",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.11",
    "@launchdarkly/observability": "^0.5.0",
    "@launchdarkly/session-replay": "^0.5.1",
    "@launchpad-ui/components": "^0.17.9",
    "@launchpad-ui/tokens": "^0.15.1",
    "@lezer/highlight": "^1.2.3",
    "@react-aria/focus": "^3.21.3",
    "@react-stately/flags": "^3.1.2",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-react": "^1.4.3",
    "@rslib/core": "^0.19.2",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^10.2.0",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/react": "^10.2.0",
    "@storybook/react-vite": "^10.1.11",
    "@storybook/test": "^9.0.0-alpha.2",
    "@tanstack/react-virtual": "^3.13.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.9",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vanilla-extract/webpack-plugin": "^2.3.25",
    "@vitest/coverage-v8": "4.0.16",
    "css-loader": "^7.1.2",
    "jsdom": "^27.4.0",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "motion": "^12.26.2",
    "oxlint": "^1.41.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.2.0",
    "storybook-addon-rslib": "^3.2.2",
    "storybook-react-rsbuild": "^3.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "launchdarkly-js-client-sdk": ">=3.9.0 <4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
