{
  "name": "@launchdarkly/toolbar",
  "version": "0.23.0-beta.1",
  "description": "A React component that provides a developer-friendly toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "react",
    "toolbar",
    "development",
    "typescript"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/js/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./dist/js/index.js",
    "./dist/static/**/*",
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/js/plugins/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "scripts": {
    "build": "rslib build && node ../../scripts/fix-base-url.cjs dist/js/index.js",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "lint": "oxlint",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.13",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.15.0",
    "@storybook/addon-docs": "^9.0.5",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^9.0.5",
    "@storybook/addon-onboarding": "^9.0.5",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/react": "^9.0.5",
    "@storybook/test": "^9.0.0-alpha.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.13",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^26.1.0",
    "launchdarkly-js-client-sdk": "^3.9.0",
    "oxlint": "^1.0.0",
    "react": "^19.1.1",
    "storybook": "^9.0.5",
    "storybook-addon-rslib": "^2.0.0",
    "storybook-react-rsbuild": "^2.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2",
    "webpack": "5.101.3"
  },
  "peerDependencies": {
    "launchdarkly-js-client-sdk": "^3.9.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@launchpad-ui/components": "0.16.9",
    "@launchpad-ui/tokens": "0.15.0",
    "@react-aria/focus": "3.21.1",
    "@react-aria/interactions": "3.25.5",
    "@react-aria/utils": "3.30.1",
    "@react-stately/utils": "3.10.8",
    "@react-types/shared": "3.32.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "motion": "^12.19.2",
    "react-aria": "3.43.2",
    "react-aria-components": "1.12.2",
    "react-hook-form": "7.59.0",
    "react-router": "7.5.2"
  }
}
