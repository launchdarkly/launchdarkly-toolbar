{
  "name": "@launchdarkly/toolbar",
  "version": "0.9.0",
  "description": "A React component that provides a developer-friendly toolbar for interacting with LaunchDarkly during development",
  "keywords": [
    "launchdarkly",
    "feature-flags",
    "react",
    "toolbar",
    "development",
    "typescript"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "author": "Catamorphic Co.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/launchdarkly-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/launchdarkly-toolbar/issues"
  },
  "homepage": "https://github.com/launchdarkly/launchdarkly-toolbar#readme",
  "main": "./dist/js/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/js/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "scripts": {
    "build": "rslib build && node scripts/fix-base-url.cjs",
    "build:storybook": "storybook build",
    "dev": "rslib build --watch",
    "release": "pnpm build && npm publish",
    "demo": "pnpm build && pnpm --filter launchdarkly-toolbar-demo dev",
    "demo:dev": "pnpm --filter launchdarkly-toolbar-demo dev",
    "demo:restart": "./scripts/dev-restart.sh",
    "demo:build": "pnpm build && pnpm --filter launchdarkly-toolbar-demo build",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:e2e:ci": "TEST_ENV=ci playwright test --config=./e2e/playwright.config.ts",
    "test:e2e:local": "TEST_ENV=local playwright test --config=./e2e/playwright.config.ts",
    "test:e2e:local:ui": "TEST_ENV=local playwright test --ui --config=./e2e/playwright.config.ts",
    "test:e2e:ci:ui": "TEST_ENV=ci playwright test --ui --config=./e2e/playwright.config.ts",
    "lint": "oxlint",
    "dev:watch": "./scripts/smart-watch-simple.sh",
    "dev:status": "./scripts/dev-status.sh",
    "dev:link": "./scripts/setup-local-links.sh",
    "dev:unlink": "./scripts/remove-local-links.sh",
    "upgrade-launchpad": "pnpm up \"@launchpad-ui/*\" react-aria \"@react-aria/*\" \"@react-types/*\" \"@react-stately/*\" react-aria-components react react-dom @types/react @types/react-dom --latest -r"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@rsbuild/core": "1.4.12",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rslib/core": "^0.11.1",
    "@storybook/addon-docs": "^9.0.5",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^9.0.5",
    "@storybook/addon-onboarding": "^9.0.5",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/react": "^9.0.5",
    "@storybook/test": "^9.0.0-alpha.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.10",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "oxlint": "^1.0.0",
    "prettier": "^3.5.3",
    "react": "^19.1.1",
    "storybook": "^9.0.5",
    "storybook-addon-rslib": "^2.0.0",
    "storybook-react-rsbuild": "^2.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@launchpad-ui/components": "^0.15.0",
    "@launchpad-ui/tokens": "^0.14.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "launchdarkly-js-client-sdk": "link:../js-client-sdk",
    "launchdarkly-react-client-sdk": "link:../react-client-sdk",
    "motion": "^12.19.2"
  },
  "pnpm": {
    "overrides": {
      "launchdarkly-js-client-sdk": "link:../js-client-sdk",
      "launchdarkly-react-client-sdk": "link:../react-client-sdk"
    }
  }
}
